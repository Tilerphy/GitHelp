git init #init an empty resp
git clone git@github.com:sunzongbao2007/first-test-git.git #download all files here
git remote add <name> git@github.com:sunzongbao2007/first-test-git.git #append a remote connection
git remote remove <name> #remove connection
git push -u <name> <branchname> # push all files to a branch in remote resp 
